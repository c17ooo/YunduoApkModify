            <TextView android:id="@id/NetworkInfo_Info" android:layout_width="fill_parent" android:layout_height="fill_parent" />
        </ScrollView>
        <LinearLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="wrap_content">
