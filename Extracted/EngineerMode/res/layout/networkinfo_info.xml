                <RadioButton android:id="@id/tx_radio_1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/force_tx_ant_radio1" />
            </RadioGroup>
            <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent" android:layout_height="wrap_content">
                <Button android:id="@id/tx_set" android:layout_width="160.0dip" android:layout_height="wrap_content" android:text="@string/force_tx_ant_set" />
                <Button android:id="@id/tx_enable" android:layout_width="160.0dip" android:layout_height="wrap_content" android:text="@string/force_tx_ant_disable" />
            </LinearLayout>
            <View android:background="#ff909090" android:layout_width="fill_parent" android:layout_height="1.0dip" android:layout_marginTop="10.0dip" />
